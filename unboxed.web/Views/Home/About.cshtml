@using System.Configuration
@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>


<div>
    <ul>
        @foreach (var setting in System.Configuration.ConfigurationManager.ConnectionStrings.Cast<ConnectionStringSettings>())
        {
            <li>@setting.Name: @setting.ConnectionString</li>
        }
    </ul>
</div>